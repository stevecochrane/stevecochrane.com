{
	"name": "stevecochrane.com",
	"version": "7.2.5",
	"description": "My personal space on the internet since 2005.",
	"author": "Steve Cochrane <steve@stevecochrane.com>",
	"license": "ISC",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/stevecochrane/stevecochrane.com.git"
	},
	"scripts": {
		"build": "webpack",
		"lint:css": "stylelint ./src/css/*.css",
		"lint:js": "eslint --ignore-path .eslintignore --cache .",
		"start": "webpack-dev-server"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": "eslint",
		"*.css": "stylelint",
		"*.{js,css,html}": "prettier --write"
	},
	"devDependencies": {
		"@babel/core": "7.3.3",
		"@babel/polyfill": "7.0.0",
		"@babel/preset-env": "7.3.1",
		"acorn": "7.1.1",
		"babel-loader": "8.0.4",
		"clean-webpack-plugin": "1.0.0",
		"copy-webpack-plugin": "6.2.0",
		"css-loader": "1.0.1",
		"eslint": "6.3.0",
		"eslint-loader": "3.0.0",
		"file-loader": "3.0.1",
		"handlebars": "4.7.6",
		"handlebars-loader": "1.7.1",
		"html-inline-css-webpack-plugin": "1.4.0",
		"html-webpack-plugin": "3.2.0",
		"husky": "1.2.0",
		"lazysizes": "5.2.1",
		"lint-staged": "10.1.3",
		"mini-css-extract-plugin": "0.4.5",
		"optimize-css-assets-webpack-plugin": "5.0.4",
		"postcss-loader": "3.0.0",
		"postcss-normalize": "7.0.1",
		"postcss-preset-env": "6.4.0",
		"prettier": "1.15.3",
		"stylelint": "9.10.1",
		"stylelint-config-recommended": "2.1.0",
		"terser-webpack-plugin": "4.2.2",
		"webpack": "4.44.2",
		"webpack-cli": "3.3.12",
		"webpack-dev-server": "3.11.0"
	}
}
